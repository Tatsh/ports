#!/usr/bin/env expect --

spawn hdiutil attach "[lindex $argv 0]" -mountpoint "[lindex $argv 1]" -private -nobrowse -noautoopen -noautofsck -noverify -readonly

expect {
	"to you. You may not copy, publish, sell, market, distribute, lend," {
		exp_send "q"
		exp_continue
	}
	"Agree Y/N?" {
		exp_send "y\r"
		exp_continue
	}
	"/dev/" {
		interact
	}
}
